{
  "version": 2,
  "name": "planificapro-gestion-presupuesto",
  "functions": {
    "api/backend.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/backend"
    },
    {
      "source": "/auth/(.*)",
      "destination": "/api/backend"
    },
    {
      "source": "/health",
      "destination": "/api/backend"
    }
  ]
}
