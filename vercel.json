{
  "version": 2,
  "name": "planificapro-gestion-presupuesto",
  "functions": {
    "api/backend.js": {
      "runtime": "@vercel/node@3.1.5"
    }
  },
  "build": {
    "env": {
      "NODE_VERSION": "20"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/backend"
    },
    {
      "source": "/auth/(.*)",
      "destination": "/api/backend"
    },
    {
      "source": "/health",
      "destination": "/api/backend"
    }
  ]
}
